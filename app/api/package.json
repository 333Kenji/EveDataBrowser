{
  "name": "@evedatabrowser/api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc -p tsconfig.build.json",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:smoke": "vitest run --config vitest.smoke.config.ts",
    "start": "node dist/index.js",
    "lint": "ESLINT_USE_FLAT_CONFIG=false eslint \"src/**/*.{ts,tsx}\" \"tests/**/*.ts\" --max-warnings=0",
    "ingest:market:esi": "tsx src/ingest/ingest-esi-market-history.ts",
    "ingest:market:report": "tsx src/ingest/report-market-history.ts",
    "ingest:market:coverage": "tsx src/ingest/market-history-coverage-report.ts"
  },
  "dependencies": {
  "@evedatabrowser/contracts": "file:../../packages/contracts",
    "@fastify/cors": "^8.5.0",
    "@fastify/postgres": "^5.0.1",
    "fastify": "^4.28.1",
    "pg": "^8.16.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "eslint": "^9.14.0",
    "@types/node": "^20.12.12",
  "@types/pg": "^8.6.6",
    "pg-mem": "^3.0.5",
    "tsx": "^4.19.2",
    "typescript": "^5.5.4",
    "vitest": "^1.6.0"
  }
}
