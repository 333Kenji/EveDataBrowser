{
  "name": "@evedatabrowser/contracts",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "generate": "tsx scripts/generate-domain.ts",
    "build": "npm run generate --silent && tsc -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "prepare": "npm run build",
    "test": "vitest run",
    "prepublishOnly": "npm run build --silent && npm run test --silent"
  },
  "dependencies": {
    "pg": "^8.12.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@types/pg": "^8.6.6",
    "tsx": "^4.20.6",
  "typescript": "5.5.4",
    "vitest": "^1.6.0"
  }
}
